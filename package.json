    "babel-eslint": "8",
    "babel-loader": "8.0.4",
    "eslint": "4.x",
    "expect.js": "0.3.1",
    "jsdom-global": "3.0.2",
    "jsdom": "13.0.0",
    "mocha": "5.2.0",
